<UserControl x:Class="AOR.View.Controls.LoadBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:AOR.View.Controls"
             mc:Ignorable="d"
             d:DesignHeight="25" d:DesignWidth="1000">
    <Grid Background="#A5A5A3">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="125"/>
            <ColumnDefinition Width="125"/>
            <ColumnDefinition Width="5*"/>
        </Grid.ColumnDefinitions>
        
        <local:ConfigFileDialog Grid.Column="0"></local:ConfigFileDialog>
        <local:OpenMidiFileDialog Grid.Column="1"></local:OpenMidiFileDialog>
    </Grid>
</UserControl>
