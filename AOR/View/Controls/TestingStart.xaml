<UserControl x:Class="AOR.View.Controls.TestingStart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:AOR.View.Controls"
             mc:Ignorable="d"
             d:DesignHeight="65" d:DesignWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
       
        <local:StartButton Grid.Column="1"></local:StartButton>
        <local:SimulatedInputFileDropDown Grid.Row="0" Grid.Column="0" x:Name= "SimulatedInputFileDropDown" ></local:SimulatedInputFileDropDown>
    </Grid>
</UserControl>
